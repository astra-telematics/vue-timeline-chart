{
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "stylelint.validate": [
    "css",
    "scss",
    "postcss",
    "vue"
  ],
  "eslint.validate": [
    "javascript",
    "typescript",
    "vue"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "vue.updateImportsOnFileMove.enabled": true
}
