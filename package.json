    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.69.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-stylistic": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.19"
  }
